#include<iostream>
using namespace std;
int main()
{
    int n,temp=0,s=0,r=0;
    cout<<"enter a number";
    cin>>n;
    temp=n;
    while(n>0)
    {
        r=n%10;
        s=s+r*r*r;
        n=n/10;
    }
    
    if(temp==s)
    cout<<"armstrong number";
    else
    cout<<"not a armstrong number";
    
}
