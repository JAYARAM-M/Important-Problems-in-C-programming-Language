#include <stdio.h>

int main()
{
    int i,j;
    int arr[100],n,s=0,count=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
    
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
        if(arr[i]==arr[j])
        count++;
        }
        printf("%d (%d)",arr[i],count);
        printf("\n");
        count=0;
    }
    
    

    return 0;
}
