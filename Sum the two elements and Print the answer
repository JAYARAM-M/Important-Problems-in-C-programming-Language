#include <stdio.h>

int main()
{
    int i;
    int arr[100],n,s=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
    
    for(i=0;i<n;i+=2)
    {
        
        s=arr[i]+arr[i+1];
        printf("%d",s);
        printf("\n");
        s=0;
        
    }
    
    

    return 0;
}
